{
	"name": "mohamed-amer-learning-npm-100",
	"type": "module",
	"version": "0.3.0",
	"main": "dist/vs-code-file-icons.js",
	"types": "dist/vs-code-file-icons.d.ts",
	"files": [
		"dist",
		"icons.cjs",
		"icons.js",
		"icons.d.ts"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/vs-code-file-icons.d.ts",
				"default": "./dist/vs-code-file-icons.js"
			}
		},
		"./icons": {
			"import": {
				"types": "./dist/icons.d.ts",
				"default": "./dist/icons.js"
			}
		}
	},
	"license": "MIT",
	"author": "Mohamed Amer",
	"scripts": {
		"build": "bun run clean && bun run build-vs-code-file-icons:esm",
		"build-vs-code-file-icons:esm": "tsup src/vs-code-file-icons.tsx src/icons.ts src/icons --dts --format esm --out-dir dist",
		"clean": "rm -rf dist",
		"lint": "tsc",
		"dev": "bun test --watch",
		"test": "bun test",
		"ci": "bun run lint && bun run test && bun run build",
		"release": "bun run ci && changeset publish"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.7",
		"@types/bun": "latest",
		"@types/react": "^18.3.4",
		"tsup": "^8.2.4",
		"typescript": "^5.0.0"
	},
	"peerDependencies": {
		"react": "^18.3.1"
	}
}
