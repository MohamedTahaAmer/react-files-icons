{
	"name": "mohamed-amer-learning-npm-100",
	"type": "module",
	"version": "0.2.0",
	"main": "dist/cjs/vs-code-file-icons.cjs",
	"module": "dist/esm/vs-code-file-icons.js",
	"types": "dist/esm/vs-code-file-icons.d.ts",
	"files": [
		"dist",
		"icons.cjs",
		"icons.js",
		"icons.d.ts"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/esm/vs-code-file-icons.d.ts",
				"default": "./dist/esm/vs-code-file-icons.js"
			},
			"require": {
				"default": "./dist/cjs/vs-code-file-icons.d.cts"
			}
		},
		"./icons": {
			"import": {
				"types": "./icons.d.ts",
				"default": "./icons.js"
			},
			"require": {
				"default": "./icons.cjs"
			}
		}
	},
	"license": "MIT",
	"author": "Mohamed Amer",
	"scripts": {
		"build": "bun run build-vs-code-file-icons:esm && bun run build-vs-code-file-icons:cjs && bun run build-icons:esm && bun run build-icons:cjs",
		"build-vs-code-file-icons:esm": "tsup src/vs-code-file-icons.ts src/icons --dts --format esm --out-dir dist/esm",
		"build-vs-code-file-icons:cjs": "tsup src/vs-code-file-icons.ts src/icons --format cjs --out-dir dist/cjs",
		"build-icons:esm": "tsup src/icons.ts --dts --format esm --out-dir dist",
		"build-icons:cjs": "tsup src/icons.ts --format cjs --out-dir dist",
		"lint": "tsc",
		"dev": "bun test --watch",
		"test": "bun test",
		"ci": "bun run lint && bun run test && bun run build",
		"release": "bun run ci && changeset publish"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.7",
		"@types/bun": "latest",
		"@types/react": "^18.3.4",
		"tsup": "^8.2.4",
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"react": "^18.3.1"
	}
}
